{
    "[python]": {
        "editor.tabSize": 4
    },
    "files.associations": {
        "*.py.in": "python",
        "*.pyi.in": "python",
        "editor.defaultFormatter": "omnilib.ufmt"
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
}
